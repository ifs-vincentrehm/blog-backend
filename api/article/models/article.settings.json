{
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "required": true
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "Published_at": {
      "type": "datetime",
      "required": true
    }
  },
  "connection": "default",
  "collectionName": "articles"
}
